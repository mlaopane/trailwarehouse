{
  "name": "trailwarehouse",
  "version": "1.0.0",
  "description": "Trail Warehouse e-shop made with Symfony 3.2.6",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "server"          : "php bin/console server:run",
    "cache-dev"       : "php bin/console cache:warmup",
    "cache-prod"      : "php bin/console cache:warmup --env=prod",
    "config"          : "php bin/console config:dump-reference",
    "container"       : "php bin/console debug:container",
    "database-create" : "php bin/console doctrine:database:create",
    "database-drop"   : "php bin/console doctrine:database:drop --force",
    "entity"          : "php bin/console doctrine:generate:entity",
    "entities"        : "php bin/console doctrine:generate:entities TrailWarehouse",
    "event"           : "php bin/console debug:event-dispatcher",
    "form"            : "php bin/console doctrine:generate:form",
    "schema-dump"     : "php bin/console doctrine:schema:update --dump-sql",
    "schema-force"    : "php bin/console doctrine:schema:update --force",
    "bundle"          : "php bin/console generate:bundle",
    "controller"      : "php bin/console generate:controller",
    "fixtures"        : "php bin/console doctrine:fixtures:load",
    "diff"            : "php bin/console doctrine:migrations:diff",
    "migrate"         : "php bin/console doctrine:migrations:migrate",
    "dql"             : "php bin/console doctrine:query:dql",
    "router"          : "php bin/console debug:router",
    "less"            : "lessc web/less/style.less web/css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mlaopane/trailwarehouse.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mlaopane/trailwarehouse/issues"
  },
  "homepage": "https://github.com/mlaopane/trailwarehouse#readme",
  "dependencies": {
    "jquery-ts": "^2.1.1"
  },
  "devDependencies": {
    "tsc": "^1.20150623.0",
    "typescript": "^2.3.4"
  }
}
